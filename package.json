{
  "name": "fonted",
  "displayName": "fonted",
  "description": "Change UI font for vscode. Just that.",
  "version": "0.0.4",
  "publisher": "degreat",
  "repository": "https://github.com/blackmann/fonted.git",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fonted.helloWorld",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fonted.enable",
        "title": "Fonted: Enable"
      },
      {
        "command": "fonted.disable",
        "title": "Fonted: Disable"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Fonted",
      "properties": {
        "fonted.font": {
          "type": "string",
          "description": "Font to replace UI font"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "node ./bundle.js",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jsdom": "^20.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "esbuild": "^0.16.8",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "jsdom": "^20.0.3"
  }
}
